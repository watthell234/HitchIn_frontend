{
  "expo": {
    "name": "HitchIn",
    "slug": "hitch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/Screens/assets/icon.png",
    "splash": {
      "image": "./src/Screens/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.watthell234.hitch"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/Screens/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.watthell234.hitch"
    },
    "web": {
      "favicon": "./src/Screens/assets/favicon.png"
    },
    "description": "Casual Carpooling for the community",
    "githubUrl": "https://github.com/watthell234/HitchIn"
  }
}
