{
  "expo": {
    "name": "HitchIn",
    "slug": "hitch",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/Screens/assets/car.png",
    "splash": {
      "image": "./src/Screens/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Casual Carpooling for the community",
    "githubUrl": "https://github.com/watthell234/HitchIn"
  }
}
